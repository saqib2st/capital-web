{
  "redirects": [
    {
      "source": "/RootNavView/:path*",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/RootNavView/OpportunityDetails",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/RootNavView/OpportunityDetails/:id",
      "destination": "/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/meta",
      "destination": "/api/meta.js"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
