{
  "rewrites": [
    {
      "source": "/RootNavView/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/RootNavView/OpportunityDetails",
      "destination": "/index.html"
    },
    {
      "source": "/RootNavView/OpportunityDetails/:id",
      "destination": "/index.html"
    },
    {
      "source": "/api/meta",
      "destination": "/api/meta.js"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
