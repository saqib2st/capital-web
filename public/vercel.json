{
  "redirects": [
    {
      "source": "/",
      "destination": "https://apps.apple.com/app/YOUR-APP-ID",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/RootNavView/OpportunityDetails",
      "destination": "/index.html"
    },
    {
      "source": "/RootNavView/OpportunityDetails/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
