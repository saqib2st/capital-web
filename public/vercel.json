{
  "redirects": [
    {
      "source": "/RootNavView/:path*",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/RootNavView/OpportunityDetails",
      "destination": "/",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/RootNavView/OpportunityDetails",
     "destination": "/" 
    },
    { "source": "/RootNavView/OpportunityDetails/id*",
     "destination": "/" 
    },
    {
      "source": "/api/meta",
      "destination": "/api/meta.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
